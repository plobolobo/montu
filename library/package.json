{
  "name": "@globolobo/quickroute-address-parser",
  "version": "1.0.1",
  "description": "NestJS library for parsing Australian addresses using TomTom Search API - works standalone or as NestJS module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./standalone": {
      "import": "./dist/standalone.js",
      "require": "./dist/standalone.cjs",
      "types": "./dist/standalone.d.ts"
    },
    "./providers/tomtom": {
      "import": "./dist/providers/tomtom.js",
      "require": "./dist/providers/tomtom.cjs",
      "types": "./dist/index.d.ts"
    },
    "./providers/google": {
      "import": "./dist/providers/google.js",
      "require": "./dist/providers/google.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run clean && tsx $(npx which rollup) -c",
    "build:watch": "npm run clean &&tsx $(npx which rollup) -c --watch",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "dev": "tsc --watch",
    "dev:standalone": "tsx $(npx which rollup) -c --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "axios": "1.11.0",
    "axios-retry": "4.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "uuid": "11.1.0",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "10.4.20",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "10.4.20",
    "@nestjs/platform-express": "11.1.5",
    "@nestjs/testing": "^10.2.10",
    "@rollup/plugin-json": "6.1.0",
    "@swc/core": "^1.3.100",
    "@types/express": "5.0.3",
    "@types/node": "^20.0.0",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^8.0.0",
    "glob": "11.0.3",
    "pkgroll": "2.15.0",
    "reflect-metadata": "0.1.14",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "tsx": "4.20.3",
    "typescript": "^5.0.0",
    "unplugin-swc": "^1.4.4",
    "vitest": "^1.0.4"
  },
  "files": [
    "dist"
  ]
}
